    }
});

$("#txtCodigo").keypress(function (event) {
    var Numeros = /^([0-9])*$/;
    if (event.which == 13 || event.keyCode == 9) {
        event.preventDefault();
        if (Numeros.test($("#txtCodigo").val())) {            
            if ($("#txtCodigo").val() == "") {
                //$("#btnConfirmarNuevoBono").focus();
            }
            else {
                Practica_Codigo_ID($("#txtCodigo").val());
            }
