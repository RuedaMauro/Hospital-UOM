@charset "utf-8";
/* CSS Document */

body,html{
  height:100%;
  margin:0px;
  font-weight:bold;
  font-family:Verdana, Geneva, sans-serif;
  margin-bottom:94px;
  overflow: hidden;
}

.BloquePrincipal{position:relative;height:100%;}
.BloqueA{position:absolute;height:100%;background-color:red;}

.Logo{height:100%;width:609px;padding:10px;
background: rgb(9,18,29);
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(-45deg,  rgba(9,18,29,1) 1%, rgba(48,91,160,1) 100%);
background: -webkit-gradient(linear, left top, right bottom, color-stop(1%,rgba(9,18,29,1)), color-stop(100%,rgba(48,91,160,1)));
background: -webkit-linear-gradient(-45deg,  rgba(9,18,29,1) 1%,rgba(48,91,160,1) 100%);
background: -o-linear-gradient(-45deg,  rgba(9,18,29,1) 1%,rgba(48,91,160,1) 100%);
background: -ms-linear-gradient(-45deg,  rgba(9,18,29,1) 1%,rgba(48,91,160,1) 100%);
background: linear-gradient(135deg,  rgba(9,18,29,1) 1%,rgba(48,91,160,1) 100%);
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#09121d', endColorstr='#305ba0',GradientType=1 );
}

.Logo img{width:100%;}

.ContenedorTurnos{position:relative;background-color:orange;margin-left:629px;text-shadow:-3px 3px 10px #333;color:#FFF;height:100%}

.Turno{
	position:relative;
	font-size:60px;
	height: 20%;
	line-height:90px;
background: rgb(57,117,191);
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(left,  rgba(57,117,191,1) 1%, rgba(41,70,137,1) 100%);
background: -webkit-gradient(linear, left top, right top, color-stop(1%,rgba(57,117,191,1)), color-stop(100%,rgba(41,70,137,1)));
background: -webkit-linear-gradient(left,  rgba(57,117,191,1) 1%,rgba(41,70,137,1) 100%);
background: -o-linear-gradient(left,  rgba(57,117,191,1) 1%,rgba(41,70,137,1) 100%);
background: -ms-linear-gradient(left,  rgba(57,117,191,1) 1%,rgba(41,70,137,1) 100%);
background: linear-gradient(to right,  rgba(57,117,191,1) 1%,rgba(41,70,137,1) 100%);
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3975bf', endColorstr='#294689',GradientType=1 );

}
.rojo{
background: rgb(185,0,0);
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  rgba(185,0,0,1) 1%, rgba(220,81,44,1) 100%);
background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,rgba(185,0,0,1)), color-stop(100%,rgba(220,81,44,1)));
background: -webkit-linear-gradient(top,  rgba(185,0,0,1) 1%,rgba(220,81,44,1) 100%);
background: -o-linear-gradient(top,  rgba(185,0,0,1) 1%,rgba(220,81,44,1) 100%);
background: -ms-linear-gradient(top,  rgba(185,0,0,1) 1%,rgba(220,81,44,1) 100%);
background: linear-gradient(to bottom,  rgba(185,0,0,1) 1%,rgba(220,81,44,1) 100%);
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#b90000', endColorstr='#dc512c',GradientType=0 );
	}
.turnosombra{
	background-image: url(../img/turnera/sombraturno.png);
	position: absolute;
	width: 100%;
	height:100%;
	background-repeat: repeat-x;
}
	
.Dato1{float:left;margin-left:15px;padding-top:2.7%}
.Dato2{float:right;margin-right:15px;padding-top:2.7%}
.clearfix{clear:both}

.Pie{position:fixed;width:100%;z-index:998;bottom:0}
.Media{overflow:hidden;height:617px;width:629px;ackground-image:url(http://www.wallsave.com/wallpapers/1024x768/paisaje/255967/paisaje-fondos-de-hermoso-pantalla-255967.jpg)}
.Media iframe{height:120%;width:200%;margin-top:-40px;margin-left:-300px;}
.BarraInferior{width:100%;height:94px;position:relative;
background: rgb(219,219,219);
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(left,  rgba(219,219,219,1) 1%, rgba(255,255,255,1) 100%);
background: -webkit-gradient(linear, left top, right top, color-stop(1%,rgba(219,219,219,1)), color-stop(100%,rgba(255,255,255,1)));
background: -webkit-linear-gradient(left,  rgba(219,219,219,1) 1%,rgba(255,255,255,1) 100%);
background: -o-linear-gradient(left,  rgba(219,219,219,1) 1%,rgba(255,255,255,1) 100%);
background: -ms-linear-gradient(left,  rgba(219,219,219,1) 1%,rgba(255,255,255,1) 100%);
background: linear-gradient(to right,  rgba(219,219,219,1) 1%,rgba(255,255,255,1) 100%);
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#dbdbdb', endColorstr='#ffffff',GradientType=1 );
}

.boxinformativo{position:absolute}
.hora{color:#FFF;font-size:70px;width:298px;background-color:#1f1f1f;height:94px;float:left;text-align:center}
.tipoContenido{color:#333;float:left;font-size:50px}
.tipoContenido img{float:left;vertical-align:middle;padding-left:20px}
.tituloNoticia{float:left;vertical-align:middle;padding:15px 0px 0px 10px}
.blackbox{display:none;background-color:rgba(0,0,0,0.9);height:100%;width:100%;position:absolute;z-index:999}
.TurnoPrincipal{margin-top:15%;height:350px;width:100%;background-color:red;color:#FFF;text-shadow:-3px 3px 10px #333;font-size:130px;text-align:center;padding-top:50px}
.NombreDoctor{width:100%;font-size:70px;padding:15px;
background: rgb(33,33,33);
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  rgba(33,33,33,1) 0%, rgba(96,96,96,1) 100%);
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(33,33,33,1)), color-stop(100%,rgba(96,96,96,1)));
background: -webkit-linear-gradient(top,  rgba(33,33,33,1) 0%,rgba(96,96,96,1) 100%);
background: -o-linear-gradient(top,  rgba(33,33,33,1) 0%,rgba(96,96,96,1) 100%);
background: -ms-linear-gradient(top,  rgba(33,33,33,1) 0%,rgba(96,96,96,1) 100%);
background: linear-gradient(to bottom,  rgba(33,33,33,1) 0%,rgba(96,96,96,1) 100%);
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#212121', endColorstr='#606060',GradientType=0 );
}

.contenedoriframe{margin-left:700px}
.iframe{height:94px;vertical-align:middle;width:100%}

.fondoturnos{position:absolute;z-index:4;width:100%;}
.fondoturnos img{width:100%;height:100%}
@media (max-width: 1800px) and (min-width: 1700px){
.ContenedorTurnos{margin-left:529px;}
.Turno{font-size:60px}	
.Logo{width:509px;}
.Media{width:529px;}
}

/*@media (max-width: 1700px) and (min-width: 1400px){*/
@media (max-width: 1360px) and (min-width: 768px){
.ContenedorTurnos{margin-left:529px;}
.Turno{font-size:45px}	
.Logo{width:509px;}
.Media{width:529px;}
}



@media (max-width: 1700px){
.TurnoPrincipal{font-size:100px;
.padding-top:2%;}
}

@media (max-width: 1400px) and (min-width: 300px) {
.ContenedorTurnos{margin-left:0px;}
.Media{display:none;}
}
	
@media (max-height: 800px) and (min-height: 500px){
.Media{height:517px}
.Turno{height:25%}
.TurnoPrincipal{margin-top:7%;}

}
