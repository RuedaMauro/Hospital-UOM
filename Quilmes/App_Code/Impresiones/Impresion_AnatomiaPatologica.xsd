                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
[PAT_NUMERO]
,S.[SOC_NRO_HC]
,MED.MED_APELLIDO_NOMBRE
,E.ESP_DESCRIPCION
,S.[SOC_APELLIDO_NOMBRES]
,MAT.PMAT_DESCRIPCION
,PRO.PPRO_DESCRIPCION
,MET.PMET_DESCRIPCION
,[PAT_ESPECIALES]
,[PAT_MACRO]
,[PAT_MICRO]
,[PAT_DIAG]
,PDIA_DESCRIPCION
,[PAT_FECHA_SALIDA]
  FROM [gest_hospitalaria].[dbo].[PATOLOGIA_ESTUDIO] as P 
  left join [gremial].[dbo].[Socios] as S on P.PAT_SOC_ID = S.[SOC_ID]
  left join [gest_hospitalaria].[dbo].[Especialidades] as E on E.ESP_ID = P.PAT_ESP_ID 
  left join [gest_hospitalaria].[dbo].[Medicos] as MED on MED.MED_ID = P.PAT_MED_ID
  left join [gest_hospitalaria].[dbo].[PATOLOGIA_MATERIAL] as MAT on MAT.PMAT_ID = P.PAT_MAT_ID
  left join [gest_hospitalaria].[dbo].[PATOLOGIA_PROCEDIMIENTO] as PRO on PRO.PPRO_ID = P.PAT_PRO_ID
  left join [gest_hospitalaria].[dbo].[PATOLOGIA_METODOS] AS MET on MET.PMET_ID = P.PAT_MET_ID
  left join [gest_hospitalaria].[dbo].[PATOLOGIA_DIAGNOSTICOS] as DIAG on DIAG.PDIA_ID = P.PAT_DIA_ID   
  where PAT_NUMERO = @PAT_NUMERO</CommandText>
